{
  "name": "Managebac Grade Calculator",
  "version": "1.3",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://opengate.managebac.com/student/*"
      ],
      "js": [
        "jquery.js",
        "content.js",
        "saveHandle.js"
      ]
    }
  ],
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "imgs/pfp.jpg",
    "48": "imgs/pfp.jpg",
    "128": "imgs/pfp.jpg"
  },
  "browser_action": {
    "default_icon": "imgs/pfp.jpg",
    "default_popup": "popup.html"
  }
}