{
    "name": "Managebac Grade Calculator",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
      "tabs"
    ],
    "content_scripts": [
      {
        "matches": ["https://opengate.managebac.com/student/*"],
        "js": ["jquery.js","content.js"]
      }
    ],
    "icons":{
      "16": "pfp.png",
      "48": "pfp.png",
      "128": "pfp.png"
    },
    "browser_action": {
      "default_icon": "pfp.png"
    }
}